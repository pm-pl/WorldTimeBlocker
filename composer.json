{
    "name": "smallkingdev/worldtimeblocker",
    "description": "A simple plugin for PocketMine-MP that freezes world time.",
    "type": "project",
    "license": "LGPL-3.0",
    "require": {
        "pocketmine/pocketmine-mp": "^4.0.0 || ^5.0.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "SmallkingDev\\WorldTimeBlocker\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
